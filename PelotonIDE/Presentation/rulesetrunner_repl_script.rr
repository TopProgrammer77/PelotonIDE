var JObject = JSON.parse(slurp(".\\LanguageConfiguration.json"));
Object.keys(JObject).forEach(function (key) {
    print(key);
    var lang = JObject[key];
    JObject[key]["ID"] = lang["GLOBAL"]["ID"];
    JObject[key]["Name"] = lang["GLOBAL"]["Name"];
    JObject[key]["Enabled"] = lang["GLOBAL"]["Enabled"];
});

CSFile.WriteAllText(".\\LanguageConfiguration2.json", JSON.stringify(JObject));
